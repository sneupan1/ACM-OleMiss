{
  "name": "acm-olemiss",
  "version": "1.0.0",
  "description": "Web application for the Ole Miss's chapter of Association of Computing Machinery (ACM)",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "env-cmd -f .env nodemon ./server/index.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "serve": "node server/index.js",
    "nelify-postbuild": "npm install --prefix client && npm run build --prefix client"
  },
  "author": "Saurav Neupane",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.3.5",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.0",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "env-cmd": "^10.1.0",
    "nodemon": "^2.0.22"
  }
}
