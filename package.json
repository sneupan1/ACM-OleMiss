{
  "name": "acm-project",
  "version": "1.0.0",
  "description": "ACM OleMiss Website",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "env-cmd -f ./config/dev.env nodemon ./server/index.js",
    "client": "env-cmd -f ./config/dev.env npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.20.0",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.7",
    "multer": "^1.4.2",
    "sharp": "^0.26.1",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "env-cmd": "^10.1.0",
    "nodemon": "^2.0.4"
  }
}
